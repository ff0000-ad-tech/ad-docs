{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
        "include": [
            "node_modules/ad-assets",
            "node_modules/ad-canvas",
            "node_modules/ad-control",
            "node_modules/ad-dates",
            "node_modules/ad-entry",
            "node_modules/ad-events",
            "node_modules/ad-external",
            "node_modules/ad-geom",
            "node_modules/ad-load",
            "node_modules/ad-ui",
            "node_modules/ad-utils",
            "node_modules/ad-video",
            "node_modules/ad-view"
        ],
        "includePattern": ".+\\.js(doc|x)?$"
    },
    "plugins": [
        "node_modules/jsdoc-babel",
        "plugins/codeblock.js"
    ],
    "babel": {
        "plugins": ["transform-class-properties"]
    },
    "templates": {
        "logoFile": "",
        "cleverLinks": false,
        "monospaceLinks": false,
        "dateFormat": "ddd MMM Do YYYY",
        "outputSourceFiles": true,
        "outputSourcePath": true,
        "systemName": "DocStrap",
        "footer": "",
        "copyright": "DocStrap Copyright Â© 2012-2015 The contributors to the JSDoc3 and DocStrap projects.",
        "navType": "vertical",
        "theme": "spacelab",
        "linenums": true,
        "collapseSymbols": false,
        "inverseNav": true,
        "protocol": "html://",
        "methodHeadingReturns": false
    },
    "opts": {
        "destination": "./docs/pages/",
        "encoding": "utf8",
        "recurse": true,
        "template": "./node_modules/ink-docstrap/template"
    }
}
